<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="myBlog.dao.PhotoDao">
	
	<select id="fetchByAid" resultType="Photo">
	select * from photo p where p.albumid=#{aid}
	</select>
	
	<select id="fetchByPid" resultType="Photo">
	select * from photo p where p.id=#{pid}
	</select>
	
	
</mapper>
